{
  "name": "backlog2slack",
  "version": "1.0.0",
  "private": true,
  "description": "Send Backlog notification to Slack",
  "author": "Minoru Nakata <minoru@sprocket.bz>",
  "license": "ISC",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "clean": "rm -rf build dist",
    "build": "eslint src && mkdir -p build dist && cp -r package.json src/* build && pushd build && npm install --production && rm package*.json && zip -r ../dist/lambda.zip . > /dev/null && popd",
    "lint": "eslint src"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "slack-node": "^0.2.0"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0"
  }
}
